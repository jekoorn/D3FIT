executable = ./n3fit.cpu.sh
arguments =  $(RUNCARD) $(IREP)
log        = ./testlogs/fit-$(IREP).log
output     = ./outfiles/fit-$(IREP).txt
error      = ./outfiles/fit-$(IREP).err

request_cpus   = 16
request_memory = 32G
request_disk   = 32G

+UseOS            = "el9"
+JobCategory      = "medium"
+accounting_group = "theorie"

queue IREP from seq 1 24 144 |
